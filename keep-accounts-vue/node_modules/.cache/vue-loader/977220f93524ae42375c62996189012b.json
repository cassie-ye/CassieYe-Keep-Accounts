v>\n                  <i class=\"el-icon-mobile-phone\" />\n                </div>\n                <div class=\"card-content-left-text\">\n                  <div class=\"first\">采购商手机号：{{ purchaseObj.buyerMobileNo }}</div>\n                  <div class=\"card-content-left-text-time\">\n                    <i class=\"el-icon-time\" />\n                    <div>{{ purchaseObj.dealTime }}</div>\n                  </div>\n                </div>\n              </div>\n              <div>\n                采购\n              </div>\n            </div>\n          </el-card>\n        </el-timeline-item>\n        <template v-if=\"allocateList.length > 0\">\n          <el-timeline-item\n            v-for=\"(item, index) in allocateList\"\n            :key=\"index\"\n            :color=\"'#409eff'\"\n            :timestamp=\"item.dealTime\"\n            placement=\"top\"\n          >\n            <el-card>\n              <div class=\"card-content\">\n                <div class=\"card-content-left\">\n                  <div>\n                    <i class=\"el-icon-mobile-phone\" />\n                  </div>\n                  <div class=\"card-content-left-text\">\n                    <div class=\"first\">摊位号：{{ item.boothNo }}</div>\n                    <div>商户名称：{{ item.proprietorName }}</div>\n                    <div class=\"card-content-left-text-time\">\n                      <i class=\"el-icon-time\" />\n                      <div>{{ item.dealTime }}</div>\n                    </div>\n                  </div>\n                </div>\n                <div>\n                  配货\n                </div>\n              </div>\n            </el-card>\n          </el-timeline-item>\n        </template>\n        <el-timeline-item :color=\"'#409eff'\" :timestamp=\"enterVenueObj.entranceTime\" placement=\"top\">\n          <el-card>\n            <div class=\"card-content\">\n              <div class=\"card-content-left\">\n                <div>\n                  <i class=\"el-ico