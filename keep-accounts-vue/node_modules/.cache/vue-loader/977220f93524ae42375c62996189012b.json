 },\n  created() {\n  },\n  methods: {\n    init(goodsDealId) {\n      this.visible = true\n      this.goodsDealId = goodsDealId\n      console.log('获取食安链条数据，goodsDealId:', goodsDealId)\n      this.getFoodSafetyChainDetails(this.goodsDealId)\n    },\n    getFoodSafetyChainDetails(params) {\n      getFoodSafetyChainDetailsAPI(params).then(res => {\n        this.purchaseObj = res.buy\n        this.allocateList = res.sell\n        this.enterVenueObj = res.entrance\n        console.log(this.purchaseObj)\n        console.log(this.allocateList)\n        console.log(this.enterVenueObj)\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.el-input-noboder .el-input__inner {\n  border: none;\n}\n\n.timeline-container {\n  padding: 20px 30px 20px 20px;\n  height: 530px;\n  overflow-y: scroll;\n\n  .card-content {\n    display: flex;\n    align-items: flex-start;\n    justify-content: space-between;\n\n    &-left {\n      display: flex;\n      // 顶部对齐\n      align-items: flex-start !important;\n\n      &-text {\n        margin-left: 20px;\n\n        .first {\n          font-size: 15px;\n          color: #666666;\n          margin-bottom: 8px;\n        }\n\n        &-time {\n          margin-top: 10px;\n          display: flex;\n          align-items: center;\n          color: #999999;\n\n          .el-icon-time {\n            margin-right: 3px;\n          }\n        }\n      }\n    }\n\n    .el-icon-mobile-phone {\n      font-size: 30px;\n    }\n  }\n}\n\n// .each-content {\n//   .top {\n//     .blue-circle {\n//       width: 13.5px;\n//       height: 13.5px;\n//       border-radius: 50%;\n//       border: 3px solid #409EFF;\n//       position: relative;\n\n//       .grey-solid {\n//         position: absolute;\n//         width: 2px;\n//         height: 14px;\n//         background-color: #C0C0C0;\n//         top: 10px;\n//         left: 3px;\n//       }\n//     }\n//   }\n// }</style>\n"]}]}